@article{haywardAgingSocialDevelopmental2015,
  title = {Aging, {{Social Developmental}}, and {{Cultural Factors}} in {{Changing Patterns}} of {{Religious Involvement Over}} a 32-{{Year Period}}: {{An Age}}\textendash{{Period}}\textendash{{Cohort Analysis}} of 80 {{Countries}}},
