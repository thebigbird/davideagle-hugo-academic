  shorttitle = {Psychosocial Stress Evoked by a Virtual Audience},
  author = {Kelly, Owen and Matheson, Kimberly and Martinez, Alejandra and Merali, Zul and Anisman, Hymie},
  year = {2007},
  month = oct,
  journal = {Cyberpsychology \& Behavior: The Impact of the Internet, Multimedia and Virtual Reality on Behavior and Society},
  volume = {10},
