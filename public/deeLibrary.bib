}

@article{halpernNeighborhoodIndividualSociodemographic2017,
  title = {Neighborhood and {{Individual Sociodemographic Characteristics Associated}} with {{Disparities}} in {{Adult Obesity}} and {{Perceptions}} of the {{Home Food Environment}}},
  author = {Halpern, Michael T. and Arena, Laura C. and Royce, Rachel A. and Soler, Robin E. and Munoz, Breda and Hennessy, Caitlin M.},
  year = {2017},
