@article{vanholtStructureEthnicViolence2012,
  title = {Structure of Ethnic Violence in {{Sudan}}: A Semi-Automated Network Analysis of Online News (2003\textendash 2010)},
  shorttitle = {Structure of Ethnic Violence in {{Sudan}}},
  author = {Van Holt, Tracy and Johnson, Jeffrey C. and Brinkley, James D. and Carley, Kathleen M. and Caspersen, Janna},
