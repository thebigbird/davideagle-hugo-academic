}

@article{wahlStandardizationDepressionMeasurement2014,
  title = {Standardization of Depression Measurement: A Common Metric Was Developed for 11 Self-Report Depression Measures},
  shorttitle = {Standardization of Depression Measurement},
  author = {Wahl, Inka and L{\"o}we, Bernd and Bjorner, Jakob Bue and Fischer, Felix and Langs, Gernot and Voderholzer, Ulrich and Aita, Stephen A. and Bergemann, Niels and Br{\"a}hler, Elmar and Rose, Matthias},
