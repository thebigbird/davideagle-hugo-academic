  shorttitle = {Putting Virtues in Context},
  author = {Kinghorn, Warren A. and Keyes, Corey L. M. and Parnell, Heather E. and Eagle, David E. and Biru, Blen M. and Amanya, Cyrilla and Vann, Vanroth and Kaza, Venkata Gopala Krishna and Tzudir, Senti and Saddo, Yohannes Benyam and Whetten, Kathryn and {Proeschold-Bell}, Rae Jean},
