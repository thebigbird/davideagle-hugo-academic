  langid = {english},
  keywords = {fair-wage theory,for-profit organizations,nonprofit organizations,organizational performance,tournament theory,wage inequality},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/nml.21085},
