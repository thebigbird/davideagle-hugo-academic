@article{fordMetabolicSyndromeIncident2008,
  title = {Metabolic {{Syndrome}} and {{Incident Diabetes}}: {{Current}} State of the Evidence},
  shorttitle = {Metabolic {{Syndrome}} and {{Incident Diabetes}}},
  author = {Ford, E. S. and Li, C. and Sattar, N.},
  year = {2008},
  month = sep,
  journal = {Diabetes Care},
  volume = {31},
  number = {9},
  pages = {1898--1904},
  issn = {0149-5992, 1935-5548},
  doi = {10.2337/dc08-0423},
  abstract = {OBJECTIVE \textemdash{} Our objective was to perform a quantitative review of prospective studies examining the association between the metabolic syndrome and incident diabetes. RESEARCH DESIGN AND METHODS \textemdash{} Using the title terms ``diabetes'' and ``metabolic syndrome'' in PubMed, we searched for articles published since 1998. RESULTS \textemdash{} Based on the results from 16 cohorts, we performed a meta-analysis of estimates of relative risk (RR) and incident diabetes. The random-effects summary RRs were 5.17 (95\% CI 3.99 \textendash{} 6.69) for the 1999 World Health Organization definition (ten cohorts); 4.45 (2.41\textendash{} 8.22) for the 1999 European Group for the Study of Insulin Resistance definition (four cohorts); 3.53 (2.84 \textendash{} 4.39) for the 2001 National Cholesterol Education Program definition (thirteen cohorts); 5.12 (3.26 \textendash{} 8.05) for the 2005 American Heart Association/National Heart, Lung, and Blood Institute definition (five cohorts); and 4.42 (3.30 \textendash 5.92) for the 2005 International Diabetes Federation definition (nine cohorts). The fixed-effects summary RR for the 2004 National Heart, Lung, and Blood Institute/American Heart Association definition was 5.16 (4.43\textendash{} 6.00) (six cohorts). Higher number of abnormal components was strongly related to incident diabetes. Compared with participants without an abnormality, estimates of RR for those with four or more abnormal components ranged from 10.88 to 24.4. Limited evidence suggests fasting glucose alone may be as good as metabolic syndrome for diabetes prediction. CONCLUSIONS \textemdash{} The metabolic syndrome, however defined, has a stronger association with incident diabetes than that previously demonstrated for coronary heart disease. Its clinical value for diabetes prediction remains uncertain.},
  langid = {english},
  file = {/Volumes/GoogleDrive/My Drive/PDFsRefs/F/Ford_et_al_2008_Metabolic_Syndrome_and_Incident_Diabetes.pdf}
}

@article{fordNuancedViewBenefits2017,
