@article{eagleBookReviewMadness2017,
  title = {Book {{Review}}: {{Madness}}: {{American Protestant Responses}} to {{Mental Illness}}, by {{Heather Vacek}}},
  shorttitle = {Madness},
  author = {Eagle, David},
  year = {2017},
  month = mar,
  journal = {Sociology of Religion},
  volume = {78},
  number = {1},
  pages = {104--105},
  issn = {1069-4404},
  doi = {10.1093/socrel/srx005},
  abstract = {Heather Vacek's book, Madness: American Protestant Responses to Mental Illness reminds us that the story of mental illness in the United States is inextricably bound to the story of religion. She traces this history through the lenses of five ``paradigmatic Protestants'' who devoted their lives to understanding and treating mental illness: Cotton Mather, Benjamin Rush, Dorothea Dix, Anton Boisen, and Karl Menninger. These figures cover the period from the early eighteenth to the late twentieth centuries.A major strength of this book is the care and attention Vacek exercises in situating each of these figures in their social and historical contexts. She avoids the temptation to lionize any of these figures. Instead, she shows that although motivated by a sincere Christian desire to alleviate suffering, each of these people was limited by popular perceptions of mental illness, institutional realities, and professional turf wars in the medical community. In addition, Vacek demonstrates that because effective treatment of mental illness by health professionals remained elusive, religious voices were central to debates about the care and treatment for the mentally ill. In other domains of medicine, religious voices occupied a much more marginalized position. A final strength of this book is the way that Vacek highlights how discussions about whether doctors, psychiatrists, or psychologists were qualified to treat mental illness\textemdash coupled with waning public support for tax dollar sponsored institutional care\textemdash created a situation where effective treatment for the mentally ill got lost. Throughout much of the time period covered, people suffering from mental illnesses often had to endure squalid, overcrowded, and inhumane environments.},
  file = {/Volumes/GoogleDrive/My Drive/PDFsRefs/E/Eagle_2017_Madness.pdf;/Users/dee4/Zotero/storage/T2QHUK55/2965305.html}
}

