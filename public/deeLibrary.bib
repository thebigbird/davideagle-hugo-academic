@article{katchungaAgeLivingUrban2012,
  title = {Age and Living in an Urban Environment Are Major Determinants of Diabetes among {{South Kivu Congolese}} Adults},
  author = {Katchunga, P. and Masumbuko, B. and Belma, M. and Munogolo Kashongwe, Z. and Hermans, M.-P. and {M'Buyamba-Kabangu}, J.-R.},
  year = {2012},
  month = oct,
  journal = {Diabetes \& Metabolism},
  volume = {38},
  number = {4},
  pages = {8},
  doi = {10/gd877f},
  abstract = {Objectives This study aimed to determine the risk factors for diabetes mellitus (DM) in the eastern part of the Democratic Republic of Congo. Methodology Multilevel sampling identified 200 households (444 adults aged {$\geq$} 20 years) from 20 neighbourhoods in the city of Bukavu, and 90 households (255 adults aged {$\geq$} 20 years) from 10 villages in the Kaziba (South Kivu) chiefdom (the South Kivu VITARAA study). DM was defined as a personal history of the disorder or a casual glycaemia greater or equal to 200 mg/dL. Standardization according to age and sample readjustment based on the urban\textendash rural distribution of the population was applied accordance with the typical Congolese population. The probability of DM was assessed by multiple logistic regressions. Results Total prevalence of DM was 3.5\%. DM was significantly more prevalent in urban areas (age-standardized prevalence: 4.0\%) than in rural areas (1.7\%). City-dwelling DM patients were characterized by higher rates of indices of abdominal obesity (P {$<$} 0.05) whereas, in rural areas, no patients were obese. In the study group as a whole, only 25.0\% of diabetic patients were obese. On multivariate analyses, only age [adjusted OR (95\% CI): 4.79 (1.60\textendash 14.25); P = 0.004] was independently associated with the prevalence of DM, while the effect of obesity was not significant [2.64 (0.99\textendash 7.02); P = 0.051]. Conclusion Age and living in an urban environment appeared to be major determinants of DM in South Kivu. Also, obesity prevalence was relatively low in these diabetic patients, confirming the peculiar, relatively lean, phenotype of type 2 DM in indigenous sub-Saharan Africans.},
  langid = {english},
  file = {/Volumes/GoogleDrive/My Drive/PDFsRefs/K/Katchunga_et_al_2012_Age_and_living_in_an_urban_environment.pdf}
}

@article{katchungaHypertensionAdultsCongolese2011,
  title = {{Hypertension in adults Congolese in South Kivu: results of the Vitaraa study}},
  author = {Katchunga, Philippe B. and {M'Buyamba-Kayamba}, Jean-Ren{\'e} and Masumbuko, Burhole E. and Lemogoum, Daniel and Kashongwe, Zacharie M and Degaute, Jean-Paul and Kabinda, Jeff M. and {M'Buyamba-Kabangu3}, Jean-Ren{\'e}},
  year = {2011},
  month = jun,
  journal = {Presse Medicale},
  volume = {40},
  number = {6},
  pages = {9},
  doi = {10/d9bx5r},
  abstract = {Objective To assess the prevalence of cardiovascular risk factors in adult and rural congolese subjects. Methods  {$\geq$}  20  years, 444 in an urban, 255 in a rural setting. We obtained anthropometric data and information on life habits and medical history in 699 people . We determined the body mass index and recorded two blood pressure measurements that were averaged for analysis. Hypertension was BP  {$\geq$}  140/90  mmHg, awareness and / or use of antihypertensive treatment. Diabetes mellitus  {$\geq$}  200  mg / dL Diabetes mellitus was self-reported . We assessed the probability of hypertension in multiple stepwise and hypertension. Results We found higher ( P {$<$} 0.001), diabetes (4.9\% vs. 3.2\%), overweight / obesity (37.6\% vs. 16.5\%), abdominal obesity \% vs. 12.9\%), use of alcohol (45\% vs. 17.6\%) and smoking (11.6\% vs. 1.2\%). Hypertension was associated ( P {$<$} 0., 05) to aging in 51.3\%, overweight / obesity in 54.5\%, diabetes in 69\%, abdominal obesity in 63.8\% and professional position (executives: 53.2\%, workers: 38.6\%). Of these hypertensive subjects, 57.5\% were unaware, 30.5\% were treated, with control achieved in only 13.6\% (17.4\% women vs. 6.9\% men; P {$<$} 0.01). In the logistic model, the probability of hypertension Increased with age (OR for age  {$>$}  55  years: 2.35; P {$<$} 0.001), overweight / obesity (2.22; P {$<$} 0.001) and diabetes mellitus (2.67; P {$<$} 0.05).       Conclusions Our results indicate a high burden of uncontrolled risk factors in the Congolese population. They highlight the need for reasonable prevention at the population level.},
  langid = {french},
  keywords = {\#nosource},
  annotation = {English Translation Option: http://www.sciencedirect.com/science/article/pii/S075549821100073X?via\%3Dihub}
}

@article{katchungaHypertensionInsulinResistance2010,
  title = {{Hypertension, insulin resistance and chronic kidney disease in a group of type 2 diabetics in South Kivu, DR Congo}},
  author = {Katchunga, Philippe and Hermans, Michel P and Manwa, Baudouin and Lepira, Francois and Kashongwe, Zacharie and {M'Buyamba-Kabangu}, Jean-Ren{\'e}},
  year = {2010},
  month = nov,
  journal = {Nephrologie \& Therapeutique},
  volume = {6},
  number = {6},
  pages = {6},
  doi = {10.1016/j.nephro.2010.04.002},
  abstract = {Objective To assess the frequency and determinants of high blood pressure (HBP) in a group of type 2 black diabetics living in the east of Democratic Republic of Congo. Methodology: The medical records of 98 diabetic patients followed at the General Reference Hospital in Bukavu between 2005 and 2007 were collected and analyzed. Hypertension was defined as blood pressure  {$\geq$}  140/90  mmHg. Insulin sensitivity (HOMA S;\%) was determined with the HOMA model, with insulin resistance (IR) representing HOMA S -1 and defined from HOMA S values {$<$} 50\%). Patients were phenotyped regarding their cardiometabolic profile using metabolic syndrome criteria (minus that for HBP). The probability of hypertension was assessed by multiple logistic regression.   results There was an overall high prevalence of HBP (59.6\%) and chronic kidney disease (66\%). In addition, hypertension was not associated with insulin resistance [regression of SBP by\% S: regression coefficient, 0.007 (-0.090-0.104) mmHg; correlation coefficient, 0.00; p  = 0.89], [regression of DBP by\% S: -0.004 (-0.053-0.045) mmHg; 0.00; 0.87]. [38.6\% versus 33.3\%, p  = 0.60]. There was no significant difference in the prevalence of metabolic syndrome  . In multivariate analysis, overweight [OR adjusted = 3.20 (95\%: 1.19-8.61), p  = 0.02] and CKD [2.49 (0.98-6.34; 0.05] were found as major determinants of hypertension. Conclusion The metabolic syndrome is poorly predictive of an insulin sensitivity in a type 2 diabetes population, in which the prevalence of hypertension was high, and that of insulin resistance low. This article describes the role of hypertension in the diagnosis of hypertension in patients with chronic kidney disease.},
  langid = {french},
  keywords = {\#nosource},
  annotation = {English translated URL: http://www.sciencedirect.com/science/article/pii/S1769725510000799?via\%3Dihub}
}

@article{katchungaKnowledgeGeneralPopulation2012,
  title = {{Knowledge of the general population about hypertension and diabetes mellitus in South Kivu, Democratic Republic of Congo}},
  author = {Katchunga, P.B. and Malanda, B. and Mweze, M.C. and Dupont, B. and {M'Buyamba-Kabangu}, J.R. and Kashongwe, Z. and Kabinda, J.M. and Buysschaert, M.},
  year = {2012},
  month = mar,
  journal = {Journal of Epidemiology and Public Health},
  volume = {60},
  number = {2},
  doi = {10/gd877d},
  abstract = {BACKGROUND: In the Democratic Republic of Congo (DRC), a country in a post-conflict period, high priority cannot be given to non-communicable diseases other than to emergencies. This certainly involves inadequacy in raising awareness for prevention of these diseases. OBJECTIVE: To evaluate the level of knowledge of the Congolese general population on hypertension and diabetes mellitus. METHODS: Responses to a questionnaire from 3\% of the general population aged 15 and older in the city of Bukavu and two rural areas: Hombo and Walungu (South Kivu, eastern DRC), recruited after stratification by ward in the city of Bukavu and a group of prone villages were expected. The questions focused on identification, testing, causes, complications and treatment of hypertension and diabetes mellitus. RESULTS: Of the 7770 respondents, screening for hypertension and diabetes mellitus affected only 14.9\% and 7.3\% of subjects respectively. Knowledge of these two conditions was generally low in the general population, although better in the subgroups of patients and those with higher socioeconomic level (P{$<$}0.05). Use of the medias was also associated with better knowledge (P{$<$}0.05). CONCLUSIONS: This study shows that knowledge about hypertension and diabetes mellitus and their testing in South Kivu is low. It is imperative that the Congolese government includes non-communicable diseases in its priorities of the millennium. Similarly, the WHO should actively contribute to screening for them in low-income countries.},
  langid = {french},
  keywords = {\#nosource},
  annotation = {English translatable link: http://www.sciencedirect.com/science/article/pii/S0398762012000533?via\%3Dihub}
}

