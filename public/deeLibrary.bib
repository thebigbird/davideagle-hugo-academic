@article{keisterDoubleOnePercent2017,
  title = {The Double One Percent: {{Identifying}} an Elite and a Super-Elite Using the Joint Distribution of Income and Net Worth},
  shorttitle = {The Double One Percent},
  author = {Keister, Lisa A. and Lee, Hang Young},
  year = {2017},
  month = aug,
  journal = {Research in Social Stratification and Mobility},
  volume = {50},
  pages = {1--12},
  issn = {0276-5624},
  doi = {10.1016/j.rssm.2017.03.004},
  abstract = {Growing inequality has heightened awareness of those at the top of the income and wealth distributions, and researchers are beginning to acknowledge the need for a way to identify top households that simultaneously accounts for their income and net worth. We contribute to the literatures on top income and wealth households by introducing a measure of top status that includes a larger number of affluent households and that explicitly accounts for the interrelationship between income and wealth. Our innovation is to start with both income and wealth holders and to divide the top into three groups: those who are top income only, those who are top net worth only, and those who are at the top of both distributions (the double one percent). Our results show that the top three groups are unique financially and demographically in ways that inform understanding of inequality and the processes that lead to membership in top income and wealth positions. Perhaps most importantly, our results identify those who are merely elite and those who occupy even more exclusive, or super elite, positions at the top of both the income and wealth distributions.},
  langid = {english},
  keywords = {Economic elites,Income inequality,Inheritance,Social mobility,The double one percent,The one percent,Wealth inequality},
  file = {/Volumes/GoogleDrive/My Drive/PDFsRefs/K/Keister_Lee_2017_The double one percent.pdf;/Users/dee4/Zotero/storage/IY8HD5KF/S0276562416301780.html}
}

