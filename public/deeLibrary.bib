@article{fowlerDynamicSpreadHappiness2008,
  title = {Dynamic Spread of Happiness in a Large Social Network: Longitudinal Analysis over 20 Years in the {{Framingham Heart Study}}},
  shorttitle = {Dynamic Spread of Happiness in a Large Social Network},
  author = {Fowler, J. H and Christakis, N. A},
  year = {2008},
