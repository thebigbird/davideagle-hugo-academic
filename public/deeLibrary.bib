@article{rossettiBurnoutCatholicClergy2013,
  title = {Burnout in {{Catholic}} Clergy: {{A}} Predictive Model Using Psychological and Spiritual Variables},
  shorttitle = {Burnout in {{Catholic}} Clergy},
  author = {Rossetti, Stephen J. and Rhoades, Colin J.},
  year = {2013},
  month = nov,
  journal = {Psychology of Religion and Spirituality},
  volume = {5},
  number = {4},
  pages = {335--341},
  publisher = {{Educational Publishing Foundation}},
  issn = {1941-1022},
  doi = {10.1037/a0033639},
  abstract = {Catholic priests are faced with innumerable demands, unrealistic expectations, and few tangible results. Moreover, in today's society, there are fewer priests, even greater demands, and a surrounding culture that is increasingly secular and apparently less supportive. Thus, burnout in Catholic clergy is commonly presumed to be high. The purpose of this study was to move beyond assumptions and explore the real extent of burnout among Roman Catholic clergy (using a standardized test of burnout) and then to explore those variables that were statistically related to burnout. A sample of 2,482 Catholic priests across the United States was given the Maslach Burnout Inventory. They scored significantly less burned out than Maslach Burnout Inventory norm groups that included a general sample and that also included those who were male or who worked in social services or medicine. When investigating those variables that were associated with burnout, exercise and taking time off were significantly correlated to lower levels of burnout; however, the effect was small. The more important variables associated with burnout were job satisfaction, inner peace, childhood psychological problems, relationship to God, and good friends. On these more important variables, priests reported high levels of health and well-being. A large majority indicated being happy as priests and reported having good friendships, satisfaction with their relationship to God, a limited history of childhood psychological problems, and a strong sense of inner peace. For Catholic priests, and likely for all people, strong psychological and spiritual support systems seem to be most important in preventing burnout, especially for those engaged in the demanding positions of caring for others. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {burnout,Catholicism,Catholics,clergy,God Concepts,inner peace,job satisfaction,Job Satisfaction,Occupational Stress,Priests,relationship to God,Roman Catholicism,Spirituality},
  file = {/Volumes/GoogleDrive/My Drive/PDFsRefs/R/Rossetti_Rhoades_2013_Burnout in Catholic clergy.pdf;/Volumes/GoogleDrive/My Drive/PDFsRefs/R/Rossetti_Rhoades_2013_Burnout in Catholic clergy2.pdf}
}

@article{rossiEvaluationHealthNutrition2007,
