@article{proeschold-bellGloryGodHuman2015,
  title = {The {{Glory}} of {{God}} Is a {{Human Being Fully Alive}}: {{Predictors}} of {{Positive Versus Negative Mental Health Among Clergy}}: {{THE GLORY OF GOD IS A HUMAN BEING FULLY ALIVE}}},
  shorttitle = {The {{Glory}} of {{God}} Is a {{Human Being Fully Alive}}},
  author = {{Proeschold-Bell}, Rae Jean and Eisenberg, Ashley and Adams, Christopher and Smith, Bruce and Legrand, Sara and Wilk, Amber},
