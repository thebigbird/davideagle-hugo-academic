@article{hebertReligiousBeliefsPractices2007,
  title = {Religious Beliefs and Practices Are Associated with Better Mental Health in Family Caregivers of Patients with Dementia: Findings from the {{REACH}} Study},
  shorttitle = {Religious {{Beliefs}} and {{Practices Are Associated With Better Mental Health}} in {{Family Caregivers}} of {{Patients With Dementia}}},
  author = {Hebert, Randy S. and Dang, Qianyu and Schulz, Richard},
