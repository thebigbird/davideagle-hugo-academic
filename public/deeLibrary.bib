@article{mischlerEffectRepeatedAcute2005,
  title = {The Effect of Repeated Acute Mental Stress on Habituation and Recovery Responses in Hemoconcentration and Blood Cells in Healthy Men},
  author = {Mischler, Katharina and Fischer, Joachim E. and Zgraggen, Lilian and Kudielka, Brigitte M. and Preckel, Daniel and {von K{\"a}nel}, Roland},
  year = {2005},
