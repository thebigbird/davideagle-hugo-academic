@article{vrijkotteOvercommitmentWorkAssociated2004,
  title = {Overcommitment to {{Work Is Associated With Changes}} in {{Cardiac Sympathetic Regulation}}},
  author = {Vrijkotte, Tanja G. M. and {van Doornen}, Lorenz J. P. and {de Geus}, Eco J. C.},
  year = {2004},
