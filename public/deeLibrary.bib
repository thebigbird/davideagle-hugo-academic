@article{vandevijverStatusReportHypertension2013,
  title = {Status Report on Hypertension in {{Africa}} - {{Consultative}} Review for the 6th {{Session}} of the {{African Union Conference}} of {{Ministers}} of {{Health}} on {{NCD}}'s},
