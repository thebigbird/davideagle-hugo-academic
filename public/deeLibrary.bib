  abstract = {The pastorate in and of itself is a challenging and demanding task to undertake. Pastoral ministry in the rural Missionary Baptist church can be especially challenging given its location and the stigma that comes along with ministry in the rural area. This dissertation will identify the potential issues of the pastoral ministry in the rural Missionary Baptist churches of the Southeast District Association of Butler and Crenshaw Counties, Alabama. These threats are potential causes of pastoral burnout and ministry failure. They are key factors in causing many to leave the pastoral ministry or causing them to leave the pastorate in the rural area. This will be a qualitative study of 20-25 pastors in the Southeast District Association of Missionary Baptist churches in the rural areas of Butler and Crenshaw Counties, Alabama. The project will primarily entail an interview of what these pastors identify are their setbacks and hindrances in ministry. It will delve into the demographics of the ministries to include: education, salary, benefits and whether or not they are now or have been bivocational and what role these elements played in how effective or ineffective they are, or have been, in ministry. In addition to the pastors, it shall also include a quantitative study of approximately 100 lay church members. This part of the study will be done by survey of each member to span across 20-25 churches in the association. This survey will ask questions of what they expect of the pastor. It will also ask questions to give statistics on how they understand pastoral ministry as an association in the areas of pastoral compensation, benefits, and educational requirements. This research gathered through the interviews will be weighed against the statistical information compiled through surveying the laity. This information will be compared and contrasted to expose the disconnect in the views of pastorate that cause the perils that may lead to ministry burnout and ministry failure. With these factors being identified, it is hoped that this study will aid new and prospective pastors in their pastoral positions in the rural Missionary Baptist churches of the Southeast District Association.},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  isbn = {9781392146026},
  langid = {english},
  school = {South University},
  keywords = {Baptist,Burnout,Missionary Baptist,Pastoral ministry,Philosophy,religion and theology,Rural church ministry,Rural poverty},
  file = {/Volumes/GoogleDrive/My Drive/PDFsRefs/S/Scott_2018_Identifying the Perils of the Pastorate of the Missionary Baptist Church in the.pdf}
}

@article{scottRuralPastorsInitiative2015,
  ids = {scottRuralPastorsInitiative2015a},
  title = {The {{Rural Pastors Initiative}}: {{Addressing Isolation}} and {{Burnout}} in {{Rural Ministry}}},
  shorttitle = {The {{Rural Pastors Initiative}}},
  author = {Scott, Greg and Lovell, Rachel},
  year = {2015},
  month = feb,
  journal = {Pastoral Psychology},
  volume = {64},
  number = {1},
  pages = {71--97},
  issn = {0031-2789, 1573-6679},
  doi = {10.1007/s11089-013-0591-z},
  abstract = {We present findings from an 18-month evaluative study in which we gathered survey and telephone interview data on 51 rural pastors who participated in an intervention designed to help them overcome some of the biggest hurdles in their professional lives (including loneliness, isolation, burnout, an imbalance between personal and professional life, and an absence of self-care activities) and to buttress the primary means by which they try to ameliorate these problems: cultivation of spiritual closeness to God. We contextualize our findings in rural environments, a reality that puts pastors in the position of ministering to their congregations but also performing an array of social services for which they never received training. Our findings suggest that rural pastors suffer an appreciable degree of loneliness and isolation. While some conditions improved over the course of the program, the participants continued to struggle with the structural and organizational barriers endemic to daily life as a rural minister. This intervention appears to have helped participants enhance their professional aptitudes, reduce their reported degree of loneliness, and connect horizontally with other congregational leaders. However, the program did not catalyze greater self-care among pastors, which may be a result of their perceiving self-care as a luxury. Finally, the data suggest that pastors attempt to make their lives better by reaching inside themselves rather than trying to connect with others. Loneliness\textemdash which may be ingrained in the job itself\textemdash remains the most robust explanatory variable, exhibiting a strong relationship with other variables such as burnout and professional excellence.},
  langid = {english},
  file = {/Users/dee4/Zotero/storage/8T9FZNTU/Scott and Lovell - 2015 - The Rural Pastors Initiative Addressing Isolation.pdf;/Users/dee4/Zotero/storage/ELYUTKYP/Scott and Lovell - 2015 - The Rural Pastors Initiative Addressing Isolation.pdf;/Volumes/GoogleDrive/My Drive/PDFsRefs/S/Scott_Lovell_2015_The Rural Pastors Initiative.pdf}
}

@article{seamanReviewInverseProbability2013,
  title = {Review of Inverse Probability Weighting for Dealing with Missing Data},
  author = {Seaman, Shaun R and White, Ian R},
  year = {2013},
  month = jun,
  journal = {Statistical Methods in Medical Research},
  volume = {22},
  number = {3},
  pages = {278--295},
  issn = {0962-2802, 1477-0334},
  doi = {10.1177/0962280210395740},
  abstract = {The simplest approach to dealing with missing data is to restrict the analysis to complete cases, i.e. individuals with no missing values. This can induce bias, however. Inverse probability weighting (IPW) is a commonly used method to correct this bias. It is also used to adjust for unequal sampling fractions in sample surveys. This article is a review of the use of IPW in epidemiological research. We describe how the bias in the complete-case analysis arises and how IPW can remove it. IPW is compared with multiple imputation (MI) and we explain why, despite MI generally being more efficient, IPW may sometimes be preferred. We discuss the choice of missingness model and methods such as weight truncation, weight stabilisation and augmented IPW. The use of IPW is illustrated on data from the 1958 British Birth Cohort.},
  langid = {english},
  file = {/Users/dee4/Zotero/storage/BJCDZA9H/Seaman and White - 2013 - Review of inverse probability weighting for dealin.pdf;/Users/dee4/Zotero/storage/L6TFXWCD/Seaman and White - 2013 - Review of inverse probability weighting for dealin.pdf}
}

@article{seamsterPredatoryInclusionEducation2017,
