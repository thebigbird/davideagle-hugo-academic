@article{panBidirectionalAssociationDepression2012,
  title = {Bidirectional {{Association Between Depression}} and {{Metabolic Syndrome}}: {{A}} Systematic Review and Meta-Analysis of Epidemiological Studies},
  shorttitle = {Bidirectional {{Association Between Depression}} and {{Metabolic Syndrome}}},
  author = {Pan, A. and Keum, N. and Okereke, O. I. and Sun, Q. and Kivimaki, M. and Rubin, R. R. and Hu, F. B.},
  year = {2012},
  month = may,
  journal = {Diabetes Care},
  volume = {35},
  number = {5},
  pages = {1171--1180},
  issn = {0149-5992, 1935-5548},
  doi = {10.2337/dc11-2055},
  abstract = {OBJECTIVEdEpidemiological studies have repeatedly investigated the association between depression and metabolic syndrome (MetS). However, the results have been inconsistent. This meta-analysis aimed to summarize the current evidence from cross-sectional and prospective cohort studies that evaluated this association. RESEARCH DESIGN AND METHODSdMEDLINE, EMBASE, and PsycINFO databases were searched for articles published up to January 2012. Cross-sectional and cohort studies that reported an association between the two conditions in adults were included. Data on prevalence, incidence, unadjusted or adjusted odds ratio (OR), and 95\% CI were extracted or provided by the authors. The pooled OR was calculated separately for cross-sectional and cohort studies using random-effects models. The I2 statistic was used to assess heterogeneity. RESULTSdThe search yielded 29 cross-sectional studies (n = 155,333): 27 studies reported unadjusted OR with a pooled estimate of 1.42 (95\% CI 1.28\textendash 1.57; I2 = 55.1\%); 11 studies reported adjusted OR with depression as the outcome (1.27 [1.07\textendash 1.57]; I2 = 60.9\%), and 12 studies reported adjusted OR with MetS as the outcome (1.34 [1.18\textendash 1.51]; I2 = 0\%). Eleven cohort studies were found (2 studies reported both directions): 9 studies (n = 26,936 with 2,316 new-onset depression case subjects) reported adjusted OR with depression as the outcome (1.49 [1.19\textendash 1.87]; I2 = 56.8\%), 4 studies (n = 3,834 with 350 MetS case subjects) reported adjusted OR with MetS as the outcome (1.52 [1.20\textendash 1.91]; I2 = 0\%). CONCLUSIONSdOur results indicate a bidirectional association between depression and MetS. These results support early detection and management of depression among patients with MetS and vice versa.},
  langid = {english},
