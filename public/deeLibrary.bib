  author = {Derose, Kathryn Pitkin and Mendel, Peter J. and Palar, Kartika and Kanouse, David E. and Bluthenthal, Ricky N. and Castaneda, Laura Werber and Corbin, Dennis E. and Dom{\'i}nguez, Blanca X. and {Hawes-Dawson}, Jennifer and Mata, Michael A. and Oden, Clyde W.},
