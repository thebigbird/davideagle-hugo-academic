  title = {Relationships between {{Sabbath Observance}} and {{Mental}}, {{Physical}}, and {{Spiritual Health}} in {{Clergy}}},
  author = {Hough, Holly and {Proeschold-Bell}, Rae Jean and Liu, Xin and Weisner, Carl and Turner, Elizabeth L. and Yao, Jia},
  year = {2019},
  month = apr,
