  title = {Profiling Development of Burnout over Eight Years: Relation with Job Demands and Resources},
  shorttitle = {Profiling Development of Burnout over Eight Years},
  author = {M{\"a}kikangas, Anne and Leiter, Michael P. and Kinnunen, Ulla and Feldt, Taru},
