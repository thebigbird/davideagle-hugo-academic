  number = {3-B},
  pages = {1596},
  address = {{US}},
  abstract = {This study proposed that clergy burnout is distinguished from burnout among helping professionals by the unique stresses of ministry and the personality characteristics of ministers. A survey was distributed to 460 ministers and helping professionals (mental health professionals, nurses, other medical professionals, and teachers) associated with two Protestant denominations. The research participants were 102 American pastors and 86 helping professionals. Situational and personality factors correlated with burnout in previous research were examined. The study utilized the Maslach Burnout Inventory (MBI) and the Spiritual Well-Being Scale as outcome measures. As hypothesized, among clergy but not helping professionals, Depersonalization (DP), a subscale of the MBI, uniquely correlated with role ambiguity ( r = .40, p {$<$} .001), "adrenaline exhaustion" (r = .29, p {$<$} .01), agreeableness ( r = -.34, p {$<$} .001), and hostility ( r = .27, p {$<$} .01). Emotional Exhaustion (EE), another MBI subscale, and DP correlated negatively with years in work ( r = -.38 and -.39, both p {$<$} .001). Among helping professionals but not among clergy, contrary to expectation, EE correlated with inadequate personal boundaries (r = .34, p {$<$} .01), exposure to crisis (r = .43, p {$<$} .001), inadequate staffing (r = .53, p {$<$} .001), and somatic complaints (r = .57, p {$<$} .001). Among both groups, EE correlated with role overload (r = .41 and .63 respectively, both p {$<$} .001), involvement in emotional triangles (r = .47 and .58, both p {$<$} .001), assault by members or clients (r =.36 and. 50, both p {$<$} .001), neurotic personality (r = .57 and .59, both p {$<$} .001), and spiritual problems (r = .53 and.52, both p {$<$} .001). DP correlated with neurotic personality (r = .49 and .32, both p {$<$} .001) and spiritual problems (r = .55, p {$<$} .001, and r = .32, p {$<$} .01). The results suggested that vague job descriptions are a factor in clergy burnout and inadequate personal boundaries, overexposure to crisis, and inadequate staffing are factors in helping professional burnout. The results also suggested that burnout includes a spiritual or existential component. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  school = {ProQuest Information \& Learning},
  keywords = {Clergy,Health Personnel,Occupational Stress,Personality Traits,Teachers},
  file = {/Users/dee4/Zotero/storage/WVCLRBLH/2002-95018-188.html}
}

@article{fosseAnalyzingAgePeriodCohortData2019,
